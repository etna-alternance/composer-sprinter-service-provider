<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>etna/sprinter-service-provider</title>
    <parser>
        <target>doc/build</target>
    </parser>
    <transformer>
        <target>doc</target>
    </transformer>
    <logging>
        <level>warn</level>
        <paths>
            <default>{APP_ROOT}/data/log/{DATE}.log</default>
            <errors>{APP_ROOT}/data/log/{DATE}.errors.log</errors>
        </paths>
    </logging>
    <transformations>
        <template name="clean" />
    </transformations>
    <files>
        <directory>src</directory>
    </files>
</phpdoc>

